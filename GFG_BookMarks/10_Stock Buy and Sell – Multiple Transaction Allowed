
QUESTION : Stock Buy and Sell â€“ Multiple Transaction Allowed

class Solution {
  public:
    int maximumProfit(vector<int> &prices) {
        int ans = 0;
        int n = prices.size();
        for(int i=0;i<n-1;i++) {
            int val = max(prices[i+1]-prices[i], 0);
            ans += val;
        }
        return ans;
    }
};